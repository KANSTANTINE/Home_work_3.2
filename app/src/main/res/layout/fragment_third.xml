<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ThirdFragment">


    <EditText
        android:id="@+id/et_fromSchool"
        android:layout_width="308dp"
        android:layout_height="56dp"
        android:layout_marginTop="46dp"
        android:hint="введите место учебы"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_fromWork"
        android:layout_width="308dp"
        android:layout_height="56dp"
        android:layout_marginTop="46dp"
        android:hint="введите место работы"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_fromSchool"/>

    <Button
        android:id="@+id/btn_third"
        android:layout_width="166dp"
        android:layout_height="52dp"
        android:layout_marginTop="16dp"
        android:text="next"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_fromWork" />

    <TextView
        android:id="@+id/tv_thirdName"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_third"
        android:layout_marginTop="18dp"/>

    <TextView
        android:id="@+id/tv_thirdSurName"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_thirdName"
        android:layout_marginTop="12dp"/>

    <TextView
        android:id="@+id/tv_thirdSex"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_thirdSurName"
        android:layout_marginTop="12dp"/>

    <TextView
        android:id="@+id/tv_thirdAge"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_thirdSex"
        android:layout_marginTop="12dp"/>




</androidx.constraintlayout.widget.ConstraintLayout>