<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".FourthFragment">

    <TextView
        android:id="@+id/tv_fourthName"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="18dp"/>

    <TextView
        android:id="@+id/tv_fourthSurName"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_fourthName"
        android:layout_marginTop="12dp"/>

    <TextView
        android:id="@+id/tv_fourthSex"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_fourthSurName"
        android:layout_marginTop="12dp"/>

    <TextView
        android:id="@+id/tv_fourthAge"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_fourthSex"
        android:layout_marginTop="12dp"/>

    <TextView
        android:id="@+id/tv_fourthFromSchool"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_fourthAge"
        android:layout_marginTop="12dp"/>

    <TextView
        android:id="@+id/tv_fourthFromWork"
        android:layout_width="308dp"
        android:layout_height="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_fourthFromSchool"
        android:layout_marginTop="12dp"/>



</androidx.constraintlayout.widget.ConstraintLayout>